{
  "presets": ["es2015", "stage-0", "react"],
  "plugins": [
    "add-module-exports",
    "transform-decorators-legacy",
    ["import", { libraryName: "antd", style: "css" }]
  ],
  "env": {
    "production": {
      "presets": ["react-optimize"],
      "plugins": [
        "babel-plugin-dev-expression"
      ]
    },
    "development": {
      "presets": ["react-hmre"]
    }
  }
}
